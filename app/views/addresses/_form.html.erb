<%= form_with(model: [@location, @address], local: true, class: "ui form") do |f| %>

<div class="field">
    <%= f.label :Street %>
    <%= f.text_field :Street %>
  </div>
  <div class="field">
    <%= f.label :city %>
    <%= f.text_field :city %>
  </div>
   <div class="field">
    <%= f.label :zip %>
    <%= f.text_field :zip %>
  </div>
   <div class="field">
    <%= f.label :state %>
    <%= f.text_field :state %>
  </div>
  <div class="actions">
    <%= f.submit nil, class: 'btn' %>
  </div>
<% end %>